
<table class="table table-striped table-bordered">
<tr>
	<th>Q. Id</th>
	<th>Question</th>
	<th>Answer</th>
	<th>Level</th>
	<th>Action</th>
</tr>
	<% @q.each do |q|%>
	<tr>
		<td><%=q.id%></td>
		<td> <%=q.question%></td>
		<td> <%=q.answer%></td>
		<td> <%=q.lvl%></td>
		<td> <a href="del_q/?del_qid=<%=q.id%>">Delete</a></td>
	</tr>
	<%end%>